<div class='input-group rounded'>
  <input
    id='searchProperty'
    type='search'
    class='form-control rounded'
    placeholder='Search for a city'
    aria-label='Search'
    aria-describedby='search-addon'
  />
  <button class='input-group-text border-0' id='search-addon'>
    <i class='fas fa-search'></i>
  </button>
</div>
<div class='container'>
  {{#each this}}
  <div class='col mb-4'>
    <div class='card h-100'>
      <div class='col mb-4'>
        <div class='card h-100'>
          <img src={{this.body.image}}onerror="this.onerror=null;this.src=' imagescoming_soon.jpeg';" />
          <div class='card-body'>
            <h5 class='card-title'>
              Address: {{body.address}}
            </h5>
            <p class='card-text-right'>
              City: {{this.info.city}}
              <br />
              Zip Code:{{info.zip}}
              <br />
              Listing Price:${{info.list_price_formatted}}
              <br />
              Square Ft:{{info.sqft}}
              <br />
              Beds:{{info.beds}}
              <br />
              Baths:{{info.baths}}
              <br />
              AirBnB ROI: {{body.airbnb_ROI}}
              <br />
              Pool:{{body.has_pool}}
              <br />
              Days on market:{{body.days_on_market}}
            </p>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<link rel='stylesheet' href='css/style.css' />
<script src='/js/search.js'></script>